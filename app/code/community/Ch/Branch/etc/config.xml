<config>
    <modules>
        <Ch_Branch>
            <version>1.0.0.0</version>
        </Ch_Branch>
    </modules>
    <global>
        <blocks>
            <ch_branch>
                <class>Ch_Branch_Block</class>
            </ch_branch>
        </blocks>
        <helpers>
            <ch_branch>
                <class>Ch_Branch_Helper</class>
            </ch_branch>
        </helpers>
        <models>
            <ch_branch>
                <class>Ch_Branch_Model</class>
                <resourceModel>ch_branch_resource</resourceModel>
            </ch_branch>
            <ch_branch_resource>
                <class>Ch_Branch_Model_Resource</class>
                <entities>
                    <branch><table>ch_branch_entity</table></branch>
                </entities>
            </ch_branch_resource>
        </models>
        <resources>
            <ch_branch_setup>
                <setup>
                    <module>Ch_Branch</module>
                    <class>Ch_Branch_Model_Resource_Setup</class>
                </setup>
            </ch_branch_setup>
        </resources>
    </global>
</config>
